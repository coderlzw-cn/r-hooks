        "@semantic-release/git": "^10.0.1",
        "@types/react": "^19.1.3",
        "commitizen": "^4.3.1",
        "cz-customizable": "^7.4.0",
        "eslint": "^9.26.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.0.0",
        "prettier": "3.5.3",
