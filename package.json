{
    "name": "@coderlzw/r-hooks",
    "version": "1.0.0",
    "description": "A collection of useful React hooks",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint .",
        "lint:check": "eslint --fix .",
